<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:fitsSystemWindows="false">

        <android.support.design.widget.AppBarLayout
            android:id="@+id/appBarLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent">

            <android.support.design.widget.CollapsingToolbarLayout
                android:id="@+id/collapsing_toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:contentScrim="?attr/colorPrimary"
                app:layout_scrollFlags="scroll|enterAlwaysCollapsed">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_5sdp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingRight="@dimen/_10sdp"
                    android:paddingTop="@dimen/_5sdp">

                    <!--<TextView-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:layout_alignParentLeft="true"-->
                        <!--android:layout_gravity="center_vertical"-->
                        <!--android:gravity="center"-->
                        <!--android:text="Searched Details"-->
                        <!--android:textColor="@color/colorWhite"-->
                        <!--android:textSize="@dimen/_12sdp"-->
                        <!--android:textStyle="bold"-->
                        <!--android:visibility="gone" />-->

                    <!--<View-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="1dp"-->
                        <!--android:layout_marginBottom="@dimen/_5sdp"-->
                        <!--android:layout_marginTop="@dimen/_5sdp"-->
                        <!--android:background="@color/colorWhite" />-->

                    <LinearLayout
                        android:id="@+id/frg_compare_data_ll_dive"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/frg_compare_data_ll_dive_one"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_lbl_ble_device_one"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_gravity="center_vertical"
                                    android:text="@string/str_device"
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_ble_device_one"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_ble_device_one"
                                    android:gravity="right"
                                    android:text="sdadfs"
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_lbl_ble_dive_one"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_gravity="center_vertical"
                                    android:text="@string/str_dives"
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_ble_dive_one"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_ble_dive_one"
                                    android:gravity="right"
                                    android:text=""
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />
                            </RelativeLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/frg_compare_data_ll_dive_two"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_lbl_ble_device_two"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_gravity="center_vertical"
                                    android:text="@string/str_device_two"
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_ble_device_two"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_ble_device_two"
                                    android:gravity="right"
                                    android:text=""
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_lbl_ble_dive_two"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_gravity="center_vertical"
                                    android:text="@string/str_dive_two"
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />

                                <TextView
                                    android:id="@+id/frg_compare_data_tv_ble_dive_two"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_ble_dive_two"
                                    android:gravity="right"
                                    android:text=""
                                    android:textColor="@color/colorWhiteLightText"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible" />
                            </RelativeLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/frg_compare_data_ll_date_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <RelativeLayout
                            android:id="@+id/frg_compare_data_ll_date_time_one"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/frg_compare_data_tv_lbl_date_time_one"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_gravity="center_vertical"
                                android:text="@string/str_date"
                                android:textColor="@color/colorWhiteLightText"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                            <TextView
                                android:id="@+id/frg_compare_data_tv_date_time_one"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_date_time_one"
                                android:gravity="right"
                                android:text=""
                                android:textColor="@color/colorWhiteLightText"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/frg_compare_data_ll_date_time_two"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/frg_compare_data_tv_lbl_date_time_two"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_gravity="center_vertical"
                                android:text="@string/str_date_two"
                                android:textColor="@color/colorWhiteLightText"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                            <TextView
                                android:id="@+id/frg_compare_data_tv_date_time_two"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_toRightOf="@+id/frg_compare_data_tv_lbl_date_time_two"
                                android:gravity="right"
                                android:text=""
                                android:textColor="@color/colorWhiteLightText"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.design.widget.CollapsingToolbarLayout>
            <android.support.design.widget.TabLayout
                android:id="@+id/frg_compare_data_smart_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:tabBackground="@color/colorTab"
                app:tabGravity="fill"
                app:tabMode="fixed"
                app:tabSelectedTextColor="@color/colorWhite"
                app:tabTextColor="@color/colorWhite" />

        </android.support.design.widget.AppBarLayout>
        <FrameLayout
            android:id="@+id/frg_compare_data_fl_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior" />
    </android.support.design.widget.CoordinatorLayout>
</RelativeLayout>