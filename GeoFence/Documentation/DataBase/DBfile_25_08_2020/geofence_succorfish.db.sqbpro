CREATE TABLE `Action_info_Table` (
	`id`	INTEGER NOT NULL,
	`action`	VARCHAR,
	`action_ID`	VARCHAR,
	`description`	VARCHAR,
	PRIMARY KEY(`id`)
);


CREATE TABLE `Geofence` (
	`id`	INTEGER NOT NULL,
	`name`	VARCHAR,
	`geofence_ID`	VARCHAR,
	`type`	VARCHAR,
	`lat`	VARCHAR,
	`long`	VARCHAR,
	`radiusOrvertices`	VARCHAR,
	`number_of_rules`	VARCHAR,
    `gsm_reporting`	VARCHAR,
    `iridium_reporting`	VARCHAR,
	`is_active`	VARCHAR,
	PRIMARY KEY(`id`)
);


CREATE TABLE `Geofence_alert_Table` (
	`id`	INTEGER NOT NULL,
	`geofence_ID`	VARCHAR,
	`Geo_name`	VARCHAR,
	`Geo_Type`	VARCHAR,
	`Breach_Type`	VARCHAR,
	`Breach_Lat`	VARCHAR,
	`Breach_Long`	VARCHAR,
	`BreachRule_ID`	VARCHAR,
	`BreachRuleValue`	VARCHAR,
	`date_Time`	VARCHAR,
	`timeStamp`	VARCHAR,
	`Rule_Name`	VARCHAR,
	`is_Read`	VARCHAR,
	`OriginalRuleValue`	VARCHAR,
	`bleAddress`	VARCHAR,
	`Message_one`	VARCHAR,
	`Message_two`	VARCHAR,
	PRIMARY KEY(`id`)
);


CREATE TABLE `Polygon_Lat_Long` (
	`id`	INTEGER NOT NULL,
	`geofence_ID`	VARCHAR,
	`lat`	VARCHAR,
	`long`	VARCHAR,
	PRIMARY KEY(`id`)
);


CREATE TABLE `Rule_info_Table` (
	`id`	INTEGER NOT NULL,
	`Rule`	VARCHAR,
	`Rule_ID`	VARCHAR,
	`description`	VARCHAR,
	PRIMARY KEY(`id`)
);


CREATE TABLE `Rules_Table` (
	`id`	INTEGER NOT NULL,
	`name`	VARCHAR,
	`geofence_ID`	VARCHAR,
	`rule_ID`	VARCHAR,
	`rule_value`	VARCHAR,
	PRIMARY KEY(`id`)
);
